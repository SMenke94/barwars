console.log("<%= @deals%>")
function addDeals(dealsListHTML) {
  const dealsList = document.getElementById('deals-list');
  dealsList.innerHTML = '';
  dealsList.insertAdjacentHTML('beforeend', dealsListHTML);
}
addDeals('<%= j render partial: "deals/list", locals: { deals: @deals} %>');
